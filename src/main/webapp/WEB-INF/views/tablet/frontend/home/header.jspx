                    <li>
                        <a href="${home_url}" title="Application Store"><span class="glyphicon glyphicon-home"><!----></span> Home</a>
                    </li>
                    <li>
                        <a id="browse-cats" class="show" href="javascript:void(0)" title="Categories"><span class="glyphicon glyphicon-th"><!----></span> Categories</a>
                    </li>
                    <li>
                        <a href="#" title="Best Apps"><span class="glyphicon glyphicon-thumbs-up"><!----></span> Best Apps</a>
                    </li>
                    <li>
                        <a href="#" title="Top Downloaded"><span class="glyphicon glyphicon-download-alt"><!----></span> Top Downloaded</a>
                    </li>
                    <li>
                        <a href="#" title="Community"><span class="glyphicon glyphicon-globe"><!----></span> Community</a>
                    </li>
                </ul>
                <c:if test="${pageContext['request'].userPrincipal != null}">
                    <ul class="navigation pull-right">
                        <li>
                            <a title="My account" href="${member_url}"><span class="glyphicon glyphicon-user"><!----></span> My Account</a>
                        </li>
                        <spring:url value="/signout.html" var="logout_url"/>
                        <li>
                            <a title="Log out" href="${logout_url}"><span class="glyphicon glyphicon-log-out"><!----></span> Log out</a>
                        </li>
                    </ul>
                </c:if>
            </div>
        </div>
        <script type="text/javascript">$(document).ready(function() {
                $('#browse-cats').click(function() {
                    if ($(this).hasClass('show')) {
                        $(this).toggleClass('show');
                        $('#cat-list').slideDown();
                        $('#header').css('border-bottom', '7px solid #71a00e');
                        $('#header .navigation-bar').css('border-bottom', '0px');
                    } else {
                        $(this).toggleClass('show');
                        $('#cat-list').slideUp();
                        $('#header').css('border-bottom', '7px');
                        $('#header .navigation-bar').css('border-bottom', '7px solid #71a00e');
                    }
                });
            });</script>
