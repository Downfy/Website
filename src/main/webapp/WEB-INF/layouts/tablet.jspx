<html xmlns:jsp="http://java.sun.com/JSP/Page" 
      xmlns:c="http://java.sun.com/jsp/jstl/core"
      xmlns:fn="http://java.sun.com/jsp/jstl/functions"
      xmlns:tiles="http://tiles.apache.org/tags-tiles" 
      xmlns:spring="http://www.springframework.org/tags" 
      xmlns:util="urn:jsptagdir:/WEB-INF/tags/util">  

    <jsp:output doctype-root-element="HTML" doctype-system="about:legacy-compat" />

    <jsp:directive.page contentType="text/html;charset=UTF-8" />  
    <jsp:directive.page pageEncoding="UTF-8" /> 
    <jsp:output omit-xml-declaration="yes"/>

    <head>
        <title>
            Mobile apps store - Downfy.com
        </title>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=8" />	
        <meta name="description" content="${description}"/>
        <meta  content="--allow-file-access-from-files" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <spring:url value="/resources/styles" var="js_path"/>
        <script src="${js_path}/jquery/jquery-1.8.1.min.js" type="text/javascript"><!-- required for FF3 and Opera --></script>
        <script src="${js_path}/bootstrap/js/bootstrap.min.js" type="text/javascript"><!-- required for FF3 and Opera --></script>
        <script src="${js_path}/bootstrap-modal.js" type="text/javascript"><!-- required for FF3 and Opera --></script>
        <script src="${js_path}/bootstrap-modalmanager.js" type="text/javascript"><!-- required for FF3 and Opera --></script>

        <link rel="stylesheet" href="${js_path}/bootstrap/css/bootstrap.min.css" type="text/css" title="ui-theme" />
        <link rel="stylesheet" href="${js_path}/bootstrap-modal.css" type="text/css" title="ui-theme" />
        <link rel="stylesheet" href="${js_path}/tablet.css" type="text/css" title="ui-theme" />

        <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
        <!--[if lt IE 9]>
          <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
